@base <http://www.example.org#> .

<http://www.example.org> rdf:type owl:Ontology ;
                          <http://purl.org/dc/elements/1.0/description> "An ontology for capturing cross-linguistic temperature terms and their meanings." ;
                          <http://purl.org/dc/elements/1.0/title> "The Linguistics of Temperature Terms Ontology" .

#################################################################
#    Annotation properties
#################################################################

###  http://example.com/ontology/hasAdditionalTerms
<http://example.com/ontology/hasAdditionalTerms> rdf:type owl:AnnotationProperty ;
                                                 rdfs:label "hasAdditionalTerms" ;
                                                 rdfs:range xsd:string ;
                                                 rdfs:domain <http://example.com/ontology/TemperatureSystem> .


###  http://example.com/ontology/hasExampleText
<http://example.com/ontology/hasExampleText> rdf:type owl:AnnotationProperty .


###  http://example.com/ontology/isMOreWarmThan
<http://example.com/ontology/isMOreWarmThan> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.0/description
<http://purl.org/dc/elements/1.0/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.0/title
<http://purl.org/dc/elements/1.0/title> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#maxCardinality
owl:maxCardinality rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#minCardinality
owl:minCardinality rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://example.com/ontology/containsWord
<http://example.com/ontology/containsWord> rdf:type owl:ObjectProperty ;
                                           owl:inverseOf <http://example.com/ontology/existsInExample> ;
                                           rdfs:domain <http://example.com/ontology/Example> ;
                                           rdfs:range <http://example.com/ontology/TemperatureLexeme> .


###  http://example.com/ontology/existsInExample
<http://example.com/ontology/existsInExample> rdf:type owl:ObjectProperty ;
                                              rdfs:domain <http://example.com/ontology/TemperatureLexeme> ;
                                              rdfs:range <http://example.com/ontology/Example> .


###  http://example.com/ontology/hasLanguage
<http://example.com/ontology/hasLanguage> rdf:type owl:ObjectProperty ;
                                          rdfs:domain <http://example.com/ontology/TemperatureLexeme> ;
                                          rdfs:range <http://dbpedia.org/resource/Language> ;
                                          owl:maxCardinality 1 .


###  http://example.com/ontology/hasPersFeelingConstruction
<http://example.com/ontology/hasPersFeelingConstruction> rdf:type owl:ObjectProperty ;
                                                         rdfs:domain <http://example.com/ontology/TemperatureSystem> ;
                                                         rdfs:range <http://example.com/ontology/PersFeelingConstruction> .


###  http://example.com/ontology/hasSemanticDomain
<http://example.com/ontology/hasSemanticDomain> rdf:type owl:ObjectProperty ;
                                                rdfs:domain <http://example.com/ontology/TemperatureLexeme> ;
                                                rdfs:range <http://example.com/ontology/SemanticDomain> .


###  http://example.com/ontology/hasTemperatureScaleType
<http://example.com/ontology/hasTemperatureScaleType> rdf:type owl:ObjectProperty ;
                                                      rdfs:domain <http://example.com/ontology/TemperatureLexeme> ;
                                                      rdfs:range <http://example.com/ontology/TemperatureScaleType> .


###  http://example.com/ontology/hasTemperatureSystemCentralityValue
<http://example.com/ontology/hasTemperatureSystemCentralityValue> rdf:type owl:ObjectProperty ;
                                                                  rdfs:domain <http://example.com/ontology/TemperatureLexeme> ;
                                                                  rdfs:range [ owl:intersectionOf ( <http://example.com/ontology/TemperatureSystemCentral>
                                                                                                    <http://example.com/ontology/TemperatureSystemPeripheral>
                                                                                                  ) ;
                                                                               rdf:type owl:Class
                                                                             ] ;
                                                                  rdfs:label "hasTemperatureSystemCentralityValue" .


###  http://example.com/ontology/isLessColdThan
<http://example.com/ontology/isLessColdThan> rdf:type owl:ObjectProperty ;
                                             owl:inverseOf <http://example.com/ontology/isMoreColdThan> ;
                                             rdfs:domain <http://example.com/ontology/TemperatureLexeme> ;
                                             rdfs:range <http://example.com/ontology/TemperatureLexeme> .


###  http://example.com/ontology/isLessWarmThan
<http://example.com/ontology/isLessWarmThan> rdf:type owl:ObjectProperty ;
                                             owl:inverseOf <http://example.com/ontology/isMoreWarmThan> ;
                                             rdfs:domain <http://example.com/ontology/TemperatureLexeme> ;
                                             rdfs:range <http://example.com/ontology/TemperatureLexeme> .


###  http://example.com/ontology/isMoreColdThan
<http://example.com/ontology/isMoreColdThan> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <http://example.com/ontology/TemperatureLexeme> ;
                                             rdfs:range <http://example.com/ontology/TemperatureLexeme> .


###  http://example.com/ontology/isMoreWarmThan
<http://example.com/ontology/isMoreWarmThan> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <http://example.com/ontology/TemperatureLexeme> ;
                                             rdfs:range <http://example.com/ontology/TemperatureLexeme> .


###  http://example.com/ontology/partOfTemperatureSystem
<http://example.com/ontology/partOfTemperatureSystem> rdf:type owl:ObjectProperty ;
                                                      rdfs:domain <http://example.com/ontology/TemperatureLexeme> ;
                                                      rdfs:range <http://example.com/ontology/TemperatureSystem> .


###  http://www.lexinfo.net/ontology/3.0/lexinfo#partOfSpeech
<http://www.lexinfo.net/ontology/3.0/lexinfo#partOfSpeech> rdf:type owl:ObjectProperty ;
                                                           rdfs:domain <http://example.com/ontology/TemperatureLexeme> ;
                                                           rdfs:range <http://www.lexinfo.net/ontology/3.0/lexinfo#PartOfSpeech> .


#################################################################
#    Data properties
#################################################################

###  http://example.com/ontology/hasComment
<http://example.com/ontology/hasComment> rdf:type owl:DatatypeProperty ;
                                         rdfs:domain <http://example.com/ontology/TemperatureLexeme> ;
                                         rdfs:range xsd:string .


###  http://example.com/ontology/hasDerivation
<http://example.com/ontology/hasDerivation> rdf:type owl:DatatypeProperty ;
                                            rdfs:domain <http://example.com/ontology/TemperatureLexeme> ;
                                            rdfs:range xsd:string .


###  http://example.com/ontology/hasExampleID
<http://example.com/ontology/hasExampleID> rdf:type owl:DatatypeProperty ;
                                           rdfs:domain <http://example.com/ontology/Example> ;
                                           rdfs:range xsd:string .


###  http://example.com/ontology/hasNumberOfCentralSystemTerms
<http://example.com/ontology/hasNumberOfCentralSystemTerms> rdf:type owl:DatatypeProperty ;
                                                            rdfs:domain <http://example.com/ontology/TemperatureSystem> ;
                                                            rdfs:range xsd:integer ;
                                                            rdfs:label "hasNumberOfCentralSystemTerms" .


###  http://example.com/ontology/hasSemanticBasicnessStatus
<http://example.com/ontology/hasSemanticBasicnessStatus> rdf:type owl:DatatypeProperty ;
                                                         rdfs:domain <http://example.com/ontology/TemperatureLexeme> ;
                                                         rdfs:range [ rdf:type rdfs:Datatype ;
                                                                      owl:oneOf [ rdf:type rdf:List ;
                                                                                  rdf:first "no" ;
                                                                                  rdf:rest [ rdf:type rdf:List ;
                                                                                             rdf:first "unknown" ;
                                                                                             rdf:rest [ rdf:type rdf:List ;
                                                                                                        rdf:first "yes" ;
                                                                                                        rdf:rest rdf:nil
                                                                                                      ]
                                                                                           ]
                                                                                ]
                                                                    ] .


###  http://example.com/ontology/hasTemperatureScaleType
<http://example.com/ontology/hasTemperatureScaleType> rdf:type owl:DatatypeProperty .


###  http://example.com/ontology/hasTranslation
<http://example.com/ontology/hasTranslation> rdf:type owl:DatatypeProperty ;
                                             rdfs:domain <http://example.com/ontology/Example> ;
                                             rdfs:range xsd:string .


###  http://example.com/ontology/hasWarmHotCollapse
<http://example.com/ontology/hasWarmHotCollapse> rdf:type owl:DatatypeProperty ;
                                                 rdfs:domain <http://example.com/ontology/TemperatureSystem> ;
                                                 rdfs:range xsd:boolean ;
                                                 rdfs:label "hasWarmHotCollapse" .


