@prefix : <https://w3id.org/def/temp_ont#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/def/temp_ont#> .

<https://w3id.org/def/temp_ont> rdf:type owl:Ontology ;
                                 <http://purl.org/dc/elements/1.0/description> "An ontology for capturing cross-linguistic temperature terms and their meanings." ;
                                 <http://purl.org/dc/elements/1.0/title> "The Linguistics of Temperature Terms Ontology" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.0/description
<http://purl.org/dc/elements/1.0/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.0/title
<http://purl.org/dc/elements/1.0/title> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#maxCardinality
owl:maxCardinality rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#minCardinality
owl:minCardinality rdf:type owl:AnnotationProperty .


###  https://w3id.org/def/temp_ont/hasAdditionalTerms
<https://w3id.org/def/temp_ont/hasAdditionalTerms> rdf:type owl:AnnotationProperty ;
                                                   rdfs:range xsd:string .


###  https://w3id.org/def/temp_ont/hasExampleText
<https://w3id.org/def/temp_ont/hasExampleText> rdf:type owl:AnnotationProperty .


###  https://w3id.org/def/temp_ont/isMOreWarmThan
<https://w3id.org/def/temp_ont/isMOreWarmThan> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.lexinfo.net/ontology/3.0/lexinfo#partOfSpeech
<http://www.lexinfo.net/ontology/3.0/lexinfo#partOfSpeech> rdf:type owl:ObjectProperty ;
                                                           rdfs:domain <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                                           rdfs:range <http://www.lexinfo.net/ontology/3.0/lexinfo#PartOfSpeech> ;
                                                           rdfs:comment "Defines that ex:TemperatureLexeme has a partOfSpeech (note lower case p) property, importinh it from the lexinfo ontology. Very relevant subclasses of the class PartOfSpeech (note capital p) include /lexinfo#NounPOS, /lexinfo#VerbPOS, /lexinfo#AdjectivePOS" .


###  https://w3id.org/def/temp_ont/containsWord
<https://w3id.org/def/temp_ont/containsWord> rdf:type owl:ObjectProperty ;
                                             owl:inverseOf <https://w3id.org/def/temp_ont/existsInExample> ;
                                             rdfs:domain <https://w3id.org/def/temp_ont/Example> ;
                                             rdfs:range <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                             rdfs:comment "Examples contain word" .


###  https://w3id.org/def/temp_ont/existsInExample
<https://w3id.org/def/temp_ont/existsInExample> rdf:type owl:ObjectProperty ;
                                                rdfs:domain <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                                rdfs:range <https://w3id.org/def/temp_ont/Example> .


###  https://w3id.org/def/temp_ont/hasLanguage
<https://w3id.org/def/temp_ont/hasLanguage> rdf:type owl:ObjectProperty ;
                                            rdfs:domain <https://w3id.org/def/temp_ont/LanguageObject> ;
                                            rdfs:range <http://dbpedia.org/resource/Language> ;
                                            rdfs:comment "Defines that ex:TemperatureLexeme have languages, and that those languages are # subclasses of http://dbpedia.org/resource/Language" ;
                                            owl:maxCardinality 1 .


###  https://w3id.org/def/temp_ont/hasPersFeelingConstruction
<https://w3id.org/def/temp_ont/hasPersFeelingConstruction> rdf:type owl:ObjectProperty ;
                                                           rdfs:domain <https://w3id.org/def/temp_ont/TemperatureSystem> ;
                                                           rdfs:range <https://w3id.org/def/temp_ont/PersFeelingConstruction> .


###  https://w3id.org/def/temp_ont/hasSemanticDomain
<https://w3id.org/def/temp_ont/hasSemanticDomain> rdf:type owl:ObjectProperty ;
                                                  rdfs:domain <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                                  rdfs:range <https://w3id.org/def/temp_ont/SemanticDomain> .


###  https://w3id.org/def/temp_ont/hasTemperatureScaleType
<https://w3id.org/def/temp_ont/hasTemperatureScaleType> rdf:type owl:ObjectProperty ;
                                                        rdfs:domain <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                                        rdfs:range <https://w3id.org/def/temp_ont/TemperatureScaleType> .


###  https://w3id.org/def/temp_ont/hasTemperatureSystemCentralityValue
<https://w3id.org/def/temp_ont/hasTemperatureSystemCentralityValue> rdf:type owl:ObjectProperty ;
                                                                    rdfs:domain <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                                                    rdfs:range [ owl:intersectionOf ( <https://w3id.org/def/temp_ont/TemperatureSystemCentral>
                                                                                                      <https://w3id.org/def/temp_ont/TemperatureSystemPeripheral>
                                                                                                    ) ;
                                                                                 rdf:type owl:Class
                                                                               ] ;
                                                                    rdfs:comment "An important distinction is whether a temperature term is domain CENTRAL or domain PERIPHERAL" ;
                                                                    rdfs:label "hasTemperatureSystemCentralityValue" .


###  https://w3id.org/def/temp_ont/isLessColdThan
<https://w3id.org/def/temp_ont/isLessColdThan> rdf:type owl:ObjectProperty ;
                                               owl:inverseOf <https://w3id.org/def/temp_ont/isMoreColdThan> ;
                                               rdfs:domain <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                               rdfs:range <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                               rdfs:comment "A TemperatureLexeme also have relative temperatures to each other. # Note that the inverse relationship means we do not at first have to  # mark both relationships when we add data - a reasoner can do that  # automatically for us" .


###  https://w3id.org/def/temp_ont/isLessWarmThan
<https://w3id.org/def/temp_ont/isLessWarmThan> rdf:type owl:ObjectProperty ;
                                               owl:inverseOf <https://w3id.org/def/temp_ont/isMoreWarmThan> ;
                                               rdfs:domain <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                               rdfs:range <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                               rdfs:comment "A TemperatureLexeme also have relative temperatures to each other. # Note that the inverse relationship means we do not at first have to  # mark both relationships when we add data - a reasoner can do that  # automatically for us" .


###  https://w3id.org/def/temp_ont/isMoreColdThan
<https://w3id.org/def/temp_ont/isMoreColdThan> rdf:type owl:ObjectProperty ;
                                               rdfs:domain <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                               rdfs:range <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                               rdfs:comment "A TemperatureLexeme also have relative temperatures to each other. # Note that the inverse relationship means we do not at first have to  # mark both relationships when we add data - a reasoner can do that  # automatically for us" .


###  https://w3id.org/def/temp_ont/isMoreWarmThan
<https://w3id.org/def/temp_ont/isMoreWarmThan> rdf:type owl:ObjectProperty ;
                                               rdfs:domain <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                               rdfs:range <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                               rdfs:comment "A TemperatureLexeme also have relative temperatures to each other. # Note that the inverse relationship means we do not at first have to  # mark both relationships when we add data - a reasoner can do that  # automatically for us" .


###  https://w3id.org/def/temp_ont/partOfTemperatureSystem
<https://w3id.org/def/temp_ont/partOfTemperatureSystem> rdf:type owl:ObjectProperty ;
                                                        rdfs:domain <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                                        rdfs:range <https://w3id.org/def/temp_ont/TemperatureSystem> .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/def/temp_ont/hasAdditionalTerms
<https://w3id.org/def/temp_ont/hasAdditionalTerms> rdf:type owl:DatatypeProperty ;
                                                   rdfs:domain <https://w3id.org/def/temp_ont/TemperatureSystem> .


###  https://w3id.org/def/temp_ont/hasComment
<https://w3id.org/def/temp_ont/hasComment> rdf:type owl:DatatypeProperty ;
                                           rdfs:domain <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                           rdfs:range xsd:string .


###  https://w3id.org/def/temp_ont/hasDerivation
<https://w3id.org/def/temp_ont/hasDerivation> rdf:type owl:DatatypeProperty ;
                                              rdfs:domain <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                              rdfs:range xsd:string ;
                                              rdfs:comment "Defines that ex:TemperatureLexeme can be derived from some other word" .


###  https://w3id.org/def/temp_ont/hasEnglishTranslation
<https://w3id.org/def/temp_ont/hasEnglishTranslation> rdf:type owl:DatatypeProperty ;
                                                      rdfs:domain <https://w3id.org/def/temp_ont/Example> ;
                                                      rdfs:range xsd:string ;
                                                      rdfs:comment "Each example has a translation into our business language, which is English" .


###  https://w3id.org/def/temp_ont/hasExampleID
<https://w3id.org/def/temp_ont/hasExampleID> rdf:type owl:DatatypeProperty ;
                                             rdfs:domain <https://w3id.org/def/temp_ont/Example> ;
                                             rdfs:range xsd:string ;
                                             rdfs:comment "Examples should have unique ids, composed by the temperature system id plus an additional unique number" .


###  https://w3id.org/def/temp_ont/hasNumberOfCentralSystemTerms
<https://w3id.org/def/temp_ont/hasNumberOfCentralSystemTerms> rdf:type owl:DatatypeProperty ;
                                                              rdfs:domain <https://w3id.org/def/temp_ont/TemperatureSystem> ;
                                                              rdfs:range xsd:integer ;
                                                              rdfs:comment "We want to record the number of system central terms that a particular system has" ;
                                                              rdfs:label "hasNumberOfCentralSystemTerms" .


###  https://w3id.org/def/temp_ont/hasSemanticBasicnessStatus
<https://w3id.org/def/temp_ont/hasSemanticBasicnessStatus> rdf:type owl:DatatypeProperty ;
                                                           rdfs:domain <https://w3id.org/def/temp_ont/TemperatureLexeme> ;
                                                           rdfs:range [ rdf:type rdfs:Datatype ;
                                                                        owl:oneOf [ rdf:type rdf:List ;
                                                                                    rdf:first "no" ;
                                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                                               rdf:first "unknown" ;
                                                                                               rdf:rest [ rdf:type rdf:List ;
                                                                                                          rdf:first "yes" ;
                                                                                                          rdf:rest rdf:nil
                                                                                                        ]
                                                                                             ]
                                                                                  ]
                                                                      ] ;
                                                           rdfs:comment " Each TemperatureLexeme has a BasicnessStatus (is it a basic temperature term?), which can be one of yes no unknown" .


###  https://w3id.org/def/temp_ont/hasTemperatureScaleType
<https://w3id.org/def/temp_ont/hasTemperatureScaleType> rdf:type owl:DatatypeProperty .


###  https://w3id.org/def/temp_ont/hasWarmHotCollapse
<https://w3id.org/def/temp_ont/hasWarmHotCollapse> rdf:type owl:DatatypeProperty ;
                                                   rdfs:domain <https://w3id.org/def/temp_ont/TemperatureSystem> ;
                                                   rdfs:range xsd:boolean ;
                                                   rdfs:comment "An important distinction is whether a temperature system collapses two levels on the warmer scale (warm+cold in one), or if it keeps them separate (warm; cold). This can be either 1 (True, yes theere is a collapse) or 0 (False, there is no collapse)" ;
                                                   rdfs:label "hasWarmHotCollapse" .


