<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/def/temp_ont#"
     xml:base="https://w3id.org/def/temp_ont"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:elements="http://purl.org/dc/elements/1.0/"
     xmlns:temp_ont="https://w3id.org/def/temp_ont/">
    <owl:Ontology rdf:about="https://w3id.org/def/temp_ont">
        <elements:description>An ontology for capturing cross-linguistic temperature terms and their meanings.</elements:description>
        <elements:title>The Linguistics of Temperature Terms Ontology</elements:title>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/elements/1.0/description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.0/description"/>
    


    <!-- http://purl.org/dc/elements/1.0/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.0/title"/>
    


    <!-- http://www.w3.org/2002/07/owl#maxCardinality -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#maxCardinality"/>
    


    <!-- http://www.w3.org/2002/07/owl#minCardinality -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#minCardinality"/>
    


    <!-- https://w3id.org/def/temp_ont/hasAdditionalTerms -->


    <owl:AnnotationProperty rdf:about="https://w3id.org/def/temp_ont/hasAdditionalTerms">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:AnnotationProperty>
    


    <!-- https://w3id.org/def/temp_ont/hasExampleText -->


    <owl:AnnotationProperty rdf:about="https://w3id.org/def/temp_ont/hasExampleText"/>
    


    <!-- https://w3id.org/def/temp_ont/isMOreWarmThan -->


    <owl:AnnotationProperty rdf:about="https://w3id.org/def/temp_ont/isMOreWarmThan"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.lexinfo.net/ontology/3.0/lexinfo#partOfSpeech -->


    <owl:ObjectProperty rdf:about="http://www.lexinfo.net/ontology/3.0/lexinfo#partOfSpeech">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:range rdf:resource="http://www.lexinfo.net/ontology/3.0/lexinfo#PartOfSpeech"/>
        <rdfs:comment>Defines that ex:TemperatureLexeme has a partOfSpeech (note lower case p) property, importinh it from the lexinfo ontology. Very relevant subclasses of the class PartOfSpeech (note capital p) include /lexinfo#NounPOS, /lexinfo#VerbPOS, /lexinfo#AdjectivePOS</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/temp_ont/containsWord -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/temp_ont/containsWord">
        <owl:inverseOf rdf:resource="https://w3id.org/def/temp_ont/existsInExample"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/Example"/>
        <rdfs:range rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:comment>Examples contain word</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/temp_ont/existsInExample -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/temp_ont/existsInExample">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:range rdf:resource="https://w3id.org/def/temp_ont/Example"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/temp_ont/hasLanguage -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/temp_ont/hasLanguage">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/LanguageObject"/>
        <rdfs:range rdf:resource="http://dbpedia.org/resource/Language"/>
        <rdfs:comment>Defines that ex:TemperatureLexeme have languages, and that those languages are # subclasses of http://dbpedia.org/resource/Language</rdfs:comment>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:maxCardinality>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/temp_ont/hasPersFeelingConstruction -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/temp_ont/hasPersFeelingConstruction">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/TemperatureSystem"/>
        <rdfs:range rdf:resource="https://w3id.org/def/temp_ont/PersFeelingConstruction"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/temp_ont/hasSemanticDomain -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/temp_ont/hasSemanticDomain">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:range rdf:resource="https://w3id.org/def/temp_ont/SemanticDomain"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/temp_ont/hasTemperatureScaleType -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/temp_ont/hasTemperatureScaleType">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:range rdf:resource="https://w3id.org/def/temp_ont/TemperatureScaleType"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/temp_ont/hasTemperatureSystemCentralityValue -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/temp_ont/hasTemperatureSystemCentralityValue">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/def/temp_ont/TemperatureSystemCentral"/>
                    <rdf:Description rdf:about="https://w3id.org/def/temp_ont/TemperatureSystemPeripheral"/>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>An important distinction is whether a temperature term is domain CENTRAL or domain PERIPHERAL</rdfs:comment>
        <rdfs:label>hasTemperatureSystemCentralityValue</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/temp_ont/isLessColdThan -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/temp_ont/isLessColdThan">
        <owl:inverseOf rdf:resource="https://w3id.org/def/temp_ont/isMoreColdThan"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:range rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:comment>A TemperatureLexeme also have relative temperatures to each other. # Note that the inverse relationship means we do not at first have to  # mark both relationships when we add data - a reasoner can do that  # automatically for us</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/temp_ont/isLessWarmThan -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/temp_ont/isLessWarmThan">
        <owl:inverseOf rdf:resource="https://w3id.org/def/temp_ont/isMoreWarmThan"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:range rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:comment>A TemperatureLexeme also have relative temperatures to each other. # Note that the inverse relationship means we do not at first have to  # mark both relationships when we add data - a reasoner can do that  # automatically for us</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/temp_ont/isMoreColdThan -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/temp_ont/isMoreColdThan">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:range rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:comment>A TemperatureLexeme also have relative temperatures to each other. # Note that the inverse relationship means we do not at first have to  # mark both relationships when we add data - a reasoner can do that  # automatically for us</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/temp_ont/isMoreWarmThan -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/temp_ont/isMoreWarmThan">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:range rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:comment>A TemperatureLexeme also have relative temperatures to each other. # Note that the inverse relationship means we do not at first have to  # mark both relationships when we add data - a reasoner can do that  # automatically for us</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/temp_ont/partOfTemperatureSystem -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/temp_ont/partOfTemperatureSystem">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:range rdf:resource="https://w3id.org/def/temp_ont/TemperatureSystem"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/def/temp_ont/hasAdditionalTerms -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/temp_ont/hasAdditionalTerms">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/TemperatureSystem"/>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/temp_ont/hasComment -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/temp_ont/hasComment">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/temp_ont/hasDerivation -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/temp_ont/hasDerivation">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Defines that ex:TemperatureLexeme can be derived from some other word</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/temp_ont/hasEnglishTranslation -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/temp_ont/hasEnglishTranslation">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/Example"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Each example has a translation into our business language, which is English</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/temp_ont/hasExampleID -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/temp_ont/hasExampleID">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/Example"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Examples should have unique ids, composed by the temperature system id plus an additional unique number</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/temp_ont/hasNumberOfCentralSystemTerms -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/temp_ont/hasNumberOfCentralSystemTerms">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/TemperatureSystem"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>We want to record the number of system central terms that a particular system has</rdfs:comment>
        <rdfs:label>hasNumberOfCentralSystemTerms</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/temp_ont/hasSemanticBasicnessStatus -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/temp_ont/hasSemanticBasicnessStatus">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/TemperatureLexeme"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>no</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>unknown</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>yes</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment> Each TemperatureLexeme has a BasicnessStatus (is it a basic temperature term?), which can be one of yes no unknown</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/temp_ont/hasTemperatureScaleType -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/temp_ont/hasTemperatureScaleType"/>
    


    <!-- https://w3id.org/def/temp_ont/hasWarmHotCollapse -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/temp_ont/hasWarmHotCollapse">
        <rdfs:domain rdf:resource="https://w3id.org/def/temp_ont/TemperatureSystem"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>An important distinction is whether a temperature system collapses two levels on the warmer scale (warm+cold in one), or if it keeps them separate (warm; cold). This can be either 1 (True, yes theere is a collapse) or 0 (False, there is no collapse)</rdfs:comment>
        <rdfs:label>hasWarmHotCollapse</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://dbpedia.org/resource/Language -->


    <owl:Class rdf:about="http://dbpedia.org/resource/Language"/>
    


    <!-- http://www.lexinfo.net/ontology/3.0/lexinfo#PartOfSpeech -->


    <owl:Class rdf:about="http://www.lexinfo.net/ontology/3.0/lexinfo#PartOfSpeech"/>
    


    <!-- http://www.w3.org/2002/07/owl#class -->


    <owl:Class rdf:about="http://www.w3.org/2002/07/owl#class"/>
    


    <!-- https://w3id.org/def/temp_ont/Example -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/Example">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/temp_ont/LanguageObject"/>
        <rdfs:comment>Examples of temperature terms in use. Examples are languages objects, and can thus be the Subjects of the hasLanguage property</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/LanguageObject -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/LanguageObject">
        <rdfs:comment>Words, phrases, examples, texts, are language objects</rdfs:comment>
        <rdfs:label>Language Object</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/OtherPOS -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/OtherPOS">
        <rdfs:subClassOf rdf:resource="http://www.lexinfo.net/ontology/3.0/lexinfo#PartOfSpeech"/>
        <rdfs:comment>For each instance of OtherPOS Consider adding to the TemperatureLexeme&apos;s rdfs:comment some text /rdfs:comment to add info on what this OtherPOS actually is</rdfs:comment>
        <rdfs:label>OtherPOS</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/PersFeelingConstruction -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/PersFeelingConstruction"/>
    


    <!-- https://w3id.org/def/temp_ont/PersFeelingConstruction_IntransitiveStrategy_ExpertumOriented -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/PersFeelingConstruction_IntransitiveStrategy_ExpertumOriented">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/temp_ont/PersFeelingConstruction"/>
        <rdfs:comment>Defines different kinds of  morphosyntactic constructions from p 137 in Koptjevskaja-Tamm, M. (2022). Semantic maps and temperature: Capturing the lexicon-grammar interface across languages. Zeitschrift für Sprachwissenschaft, 41(1), 125-177. https://doi.org/10.1515/zfs-2021-2042</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/PersFeelingConstruction_IntransitiveStrategy_NonOriented -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/PersFeelingConstruction_IntransitiveStrategy_NonOriented">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/temp_ont/PersFeelingConstruction"/>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/PersFeelingConstruction_Other -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/PersFeelingConstruction_Other">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/temp_ont/PersFeelingConstruction"/>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/PersFeelingConstruction_TransitiveStrategy_ExperiencerOriented -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/PersFeelingConstruction_TransitiveStrategy_ExperiencerOriented">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/temp_ont/PersFeelingConstruction"/>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/PersFeelingConstruction_TransitiveStrategy_ExpertumOriented -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/PersFeelingConstruction_TransitiveStrategy_ExpertumOriented">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/temp_ont/PersFeelingConstruction"/>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/PersFeelingConstruction_TransitiveStrategy_NonOriented -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/PersFeelingConstruction_TransitiveStrategy_NonOriented">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/temp_ont/PersFeelingConstruction"/>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/SemanticDomain -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/SemanticDomain">
        <rdfs:comment>Defines that TemperatureLexeme has a semantic domain</rdfs:comment>
        <rdfs:label>Semantic Domain</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/StateVerbPOS -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/StateVerbPOS">
        <rdfs:subClassOf rdf:resource="http://www.lexinfo.net/ontology/3.0/lexinfo#PartOfSpeech"/>
        <rdfs:comment>the lexinfo ontology does not define State verbs, nor does it have a general “other POS” option, so we add those her</rdfs:comment>
        <rdfs:label>StateVerbPOS</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/TemperatureLexeme -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/TemperatureLexeme">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/temp_ont/LanguageObject"/>
        <rdfs:label>Temperature Lexeme</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/TemperatureScaleType -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/TemperatureScaleType">
        <rdfs:comment>A TemperatureLexeme can belong either to a hot scale or to a cold scale or to a # neutral scale (e.g. lukewarm)</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/TemperatureScaleTypeColdness -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/TemperatureScaleTypeColdness">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/temp_ont/TemperatureScaleType"/>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/TemperatureScaleTypeNeutral -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/TemperatureScaleTypeNeutral">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/temp_ont/TemperatureScaleType"/>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/TemperatureScaleTypeWarmth -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/TemperatureScaleTypeWarmth">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/temp_ont/TemperatureScaleType"/>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/TemperatureSystem -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/TemperatureSystem"/>
    


    <!-- https://w3id.org/def/temp_ont/TemperatureSystemCentral -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/TemperatureSystemCentral"/>
    


    <!-- https://w3id.org/def/temp_ont/TemperatureSystemPeripheral -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/TemperatureSystemPeripheral"/>
    


    <!-- https://w3id.org/def/temp_ont/hasComment -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/hasComment"/>
    


    <!-- https://w3id.org/def/temp_ont/temperature_air -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/temperature_air">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/temp_ont/SemanticDomain"/>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/temperature_ambient -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/temperature_ambient">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/temp_ont/SemanticDomain"/>
        <rdfs:comment>Ambient temperatures  relate to circumstances or places</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/temperature_ambient_NR -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/temperature_ambient_NR">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/temp_ont/temperature_ambient"/>
        <rdfs:comment>temperatures related to certain circumstances. Quasi-referential ambient temperature terms involve some kind of referent (the weather was cold, the room was cold), in contrast with non-referential (NR) ambient temperature terms that do not (it was cold in the room)</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/temperature_ambient_QR -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/temperature_ambient_QR">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/temp_ont/temperature_ambient"/>
        <rdfs:comment>temperatures related to certain circumstances. Quasi-referential ambient temperature terms involve some kind of referent (the weather was cold, the room was cold), in contrast with non-referential ambient temperature terms that do not (it was cold in the room)</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/temperature_general -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/temperature_general">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/temp_ont/SemanticDomain"/>
    </owl:Class>
    


    <!-- https://w3id.org/def/temp_ont/temperature_personalfeeling -->


    <owl:Class rdf:about="https://w3id.org/def/temp_ont/temperature_personalfeeling">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/temp_ont/SemanticDomain"/>
